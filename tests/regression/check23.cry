pad : {msgLen, padding, contentLen, chunks}
    ( contentLen == msgLen + 65
    , chunks == (contentLen + 511) / 512
    , 64 >= width msgLen
    , padding == (512 - contentLen % 512) % 512
    ) => [msgLen] -> [chunks * 512]
pad msg = msg # [True] # (zero:[padding]) # sz  //or: (`msgLen:[64])
      where  sz : [64]
             sz = (length msg):[64]

