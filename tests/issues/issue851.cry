type base t = { zero: t }

B: base Bool
B = { zero = False }

foo: {t,k} base t -> [k]t
foo b = repeat b.zero

bar: {t,k} (k >= 1) => [k]t -> [k]t -> [k]t -> [k]t
bar p q m = p

baz: {k,t} (k >= 1) => [k]t -> [k]t -> [k]t
baz p q = q

property problem p =
  p != foo B ==> bar p (baz p 0x03) 0x03 == foo B
